{
	"parser": "babel-eslint",
	"extends": [
	  "eslint:recommended",
	  "standard",
	  "prettier"
	],
	"env": {
	  "jest": true,
	  "browser": true,
	  "jasmine": true
	},
	"plugins": [
	  "prettier"
	],
	"rules": {
	  "comma-dangle": "off",
	  "no-console": "off",
	  "prettier/prettier": [
		"error",
		{
		  "printWidth": 100,
		  "semi": true,
		  "singleQuote": true
		}
	  ]
	}
  }
